{
  "manifest_version": 2,

  "name": "LeetHub",
  "description": "LeetHub is your repo for all LeetCode.com problem solutions.",
  "version": "0.0.1",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "index.html",
   "default_title": "Click to active LeetHub"
  },
  "permissions": [
   "activeTab"
   ]
}